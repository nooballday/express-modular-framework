{
  "name": "nextgen-spaj-express",
  "version": "1.0.0",
  "description": "Web API for Prisma BRILife APp",
  "main": "index.js",
  "author": "Naufal, Ujang",
  "license": "MIT",
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "scripts": {
    "babel-node": "babel-node --presets=es2015",
    "start:dev": "nodemon --exec npm run babel-node -- ./src/index.js",
    "start:prod": "yarn build && node -r dotenv/config dist/index.js",
    "build": "babel src --out-dir dist",
    "test": "./node_modules/.bin/mocha -r dotenv/config 'src/**/*.spec.js' js:babel-core/register"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^4.1.2",
    "mocha": "^5.2.0",
    "winston": "^3.0.0",
    "winston-daily-rotate-file": "^3.3.2",
    "rotating-file-stream": "^1.3.7"
  },
  "dependencies": {
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "morgan": "^1.9.1"
  }
}